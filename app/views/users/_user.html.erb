<table class="table table-condensed" style="width:50%" align = "center">
<thead>
  <tr>
    <th>Avatar</th>
    <th>Profile</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= gravatar_for (user) %></td>
      <td><%= user.id %>
          <%= user.name %>
      </td>
    </tr>
  <% end %>
</tbody>
</table>
<%= will_paginate %>